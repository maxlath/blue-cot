{
  "name": "blue-cot",
  "description": "bluebird promise-based CouchDB library with no surprises (in a good way)",
  "keywords": [
    "couch",
    "couchdb",
    "promise",
    "promises",
    "bluebird"
  ],
  "scripts": {
    "build": "coffee -c cot.coffee",
    "watch": "coffee -cw cot.coffee",
    "test": "mocha --compilers coffee:coffee-script/register",
    "test-watch": "mocha --compilers coffee:coffee-script/register --watch",
    "benchmark": "coffee benchmark/benchmark.coffee",
    "prepublish": "npm build && npm test"
  },
  "version": "2.0.0",
  "main": "cot",
  "dependencies": {
    "bluereq": "^2.1.0"
  },
  "author": {
    "name": "maxlath",
    "url": "http://maxlath.eu"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/maxlath/blue-cot"
  },
  "forkedFrom": "http://github.com/willconant/cot-node",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "bluebird": "^3.0.5",
    "chai": "^1.9.2",
    "coffee-errors": "^0.8.6",
    "coffee-script": "^1.10.0",
    "cot": "0.0.6",
    "mocha": "^2.0.1"
  }
}
