{
  "name": "blue-cot",
  "description": "bluebird promise-based CouchDB library with no surprises (in a good way)",
  "keywords": [
    "couch",
    "couchdb",
    "promise",
    "promises",
    "bluebird"
  ],
  "scripts": {
    "build": "coffee -c cot.coffee",
    "watch": "coffee -cw cot.coffee",
    "build-test": "coffee -c test/*.coffee",
    "watch-test": "coffee -cw test/*.coffee",
    "test": "npm run build-test && mocha"
  },
  "version": "1.0.0",
  "main": "cot",
  "dependencies": {
    "bluereq": "^0.3.0"
  },
  "author": {
    "name": "maxlath",
    "url": "http://maxlath.eu"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/maxlath/blue-cot"
  },
  "forkedFrom": "http://github.com/willconant/cot-node",
  "devDependencies": {
    "bluebird": "^3.0.5",
    "chai": "^1.9.2",
    "coffee-script": "^1.10.0",
    "mocha": "^2.0.1"
  }
}
